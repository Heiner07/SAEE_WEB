@inject ProfesoresServices ServicioProfesores
@inject NavigationManager Navigation

<Form Editando="false" OnValidSubmit="AgregarProfesor" Titulo="Agregando profesor" TextoBoton="Agregar" Profesor="@profesor" />

@code {
    Profesores profesor = new Profesores();

    async Task AgregarProfesor(){
        profesor.Administrador = false;
        await ServicioProfesores.PostProfesores(profesor);
        Navigation.NavigateTo("profesores/1", true);
    }
}
